             </div>
             <div class="label"><%= playerData.stats.p10.score.label %> </div>
             <hr>
            
             
            </div>
 
           </div>
           <div class="ui inverted statistics">

  
            <div class="statistic">
   
              <div class="value">
                <%= playerData.stats.p10.trnRating.displayValue %>
              </div>
              <div class="label"><%= playerData.stats.p10.trnRating.label %> </div>
              <hr>
            </div>

 
           </div>


           

           
         </div>
         

         
      
    
    </div>
    <!-- end of duo -->

    <!--squad-->
    <div class="mode-states">
      <div class="card-info card-info--squad">
        <div class="mode-name">
          Squad
        </div>
         <div class="matches-played">
           <%= playerData.stats.p9.matches.displayValue %> Matches
           
